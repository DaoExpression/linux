; internal LAN

$ORIGIN .

$TTL 60

lan	IN		SOA	obr.lan. root.obr.lan. (
				2024122100	; serial
				3h		; refresh
				15m		; retry
				2w		; expire
				60		; minimum TTL
			)

			IN	NS		obr.lan.

$ORIGIN lan.

